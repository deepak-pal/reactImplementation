{
"extends":["eslint:recommended",
    "plugins:import/errors",
    "plugins:react/recommended",
    "plugins:jsx-a11y/recomended", 
    "prettier"],
    "rules": {
        "react/prop-type": 0,
        "no-console":1
    },
"plugins":["react", "import", "jsx-a11y"],
"parserOptions":{
    "emmaVersion":2018,
    "sourceType": "module",
    "ecmaFeatures":{
        "jsx":true
    }
},
"env":{
    "es6":true,
    "browser":true,
    "node":true
},
"settings": {
    "react":"detect"
}

}